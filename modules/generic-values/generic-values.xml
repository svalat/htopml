<htopml>
	<config>
		<root-mount-point>/generic</root-mount-point>
		<name>Generic values</name>
		<!--<icon>memory.png</icon>-->
	</config>
	<graphs>
		<graph-lines>
			<container>generic-values</container>
			<title>Generic values</title>
			<xlabel>Time</xlabel>
